<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean class="com.springcore.reference.B" name="bref">
		<property name="y" value="20" />

	</bean>

<!-- putting value as element -->
	<bean class="com.springcore.reference.A" name="aref">
		<property name="x" value="11" />
		<property name="ob">
			<ref bean="bref" />
		</property>
	</bean>
	
	<!-- putting value as attribute 
	<bean class="com.springcore.reference.A" name="aref">
	<property name="ob" ref="aref">
	</property></bean> 
	-->
	
	<!-- putting value using p schema
	<bean class="com.springcore.reference.A" name="aref" p:x="11" p:ob-ref="20"/>
	-->

	 
</beans>
 

 
 
 
